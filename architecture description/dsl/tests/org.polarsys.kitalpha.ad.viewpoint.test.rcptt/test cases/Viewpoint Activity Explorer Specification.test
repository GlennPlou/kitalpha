get-view "Project Explorer" | get-tree | select "org.polarsys.kitalpha.vp.datatest.vpdsl/model/datatest.spec.vptext" 
    | double-click
with [get-editor "datatest.spec.vptext" | get-text-viewer] {
    set-caret-pos 13 30
    key-type Enter
    key-type "M1+SPACE"
    key-type Down -times 4
    key-type Enter
}
get-window "Viewpoint Activity Explorer File Creation Wizard" | get-button Finish | click
get-view "Project Explorer" | get-tree | select "data/Activity Explorer Specification" | double-click
with [get-editor "Activity Explorer Specification" | get-text-viewer] {
    set-caret-pos 2 1
    select-range 1 1 128 2
    get-menu Copy | click
}
get-tab-folder -index 2 | get-tab-item "datatest.activityexplorer.vptext" | click
with [get-editor "datatest.activityexplorer.vptext" | get-text-viewer] {
    set-caret-pos 1 2
    select-range 1 1 11 2
    get-menu Paste | click
}
get-menu "File/Save All" | click
get-view Outline | get-tree | get-item "datatest.activityexplorer" | get-property childCount | equals 4 | verify-true
get-view "Project Explorer" | get-tree | select "org.polarsys.kitalpha.vp.datatest.vpdsl/model/datatest.vpdesc" 
    | double-click
