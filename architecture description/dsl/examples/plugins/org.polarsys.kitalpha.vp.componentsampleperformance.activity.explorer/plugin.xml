<?eclipse version="3.4"?>
<plugin>
<extension
      point="org.eclipse.amalgam.explorer.activity.ui.sectionsProvider">

		<Section
			   expanded="true"
			   filtering="true"
			   id="org.polarsys.kitalpha.vp.ComponentSamplePerformance.start.stop.section"	   
			   index="2"
			   name="Component Sample Performance"
			   pageId="org.polarsys.kitalpha.vp.componentsample.activity.explorer.architecture">
			<Description>
				&lt;p&gt;Reference/Unreference Component sample Performance Viewpoint&lt;/p&gt;
			</Description>
			<Activity
				class="org.polarsys.kitalpha.vp.componentsampleperformance.activity.explorer.activity.ReferenceComponentSamplePerformanceViewpointActivity"
				id="org.polarsys.kitalpha.vp.ComponentSamplePerformance.start.activity"
				index="1" 
				image="icons/reference.png"
				name="Reference">
			<Description>
				Reference Component sample Performance Viewpoint
			</Description>
			<Predicate
				class="org.polarsys.kitalpha.vp.componentsampleperformance.activity.explorer.predicate.AReferenceComponentSamplePerformanceViewpointPredicate">
			</Predicate>
			</Activity>

			<Activity
				class="org.polarsys.kitalpha.vp.componentsampleperformance.activity.explorer.activity.UnreferenceComponentSamplePerformanceViewpointActivity"
				id="org.polarsys.kitalpha.vp.ComponentSamplePerformance.start.activity"
				index="2" 
				image="icons/unreference.png"
				name="Unreference">
			<Description>
				Unreference Component sample Performance Viewpoint
			</Description>
			<Predicate
				class="org.polarsys.kitalpha.vp.componentsampleperformance.activity.explorer.predicate.AUnreferenceComponentSamplePerformanceViewpointPredicate">
			</Predicate>
			</Activity>
		</Section>
</extension>
</plugin>
