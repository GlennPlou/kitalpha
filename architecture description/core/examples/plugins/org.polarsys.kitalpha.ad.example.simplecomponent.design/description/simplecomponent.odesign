<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/description/style/1.1.0" name="simplecomponent" version="7.0.0">
  <ownedViewpoints name="simplecomponent" modelFileExtension="simplecomponent">
    <ownedRepresentations xsi:type="description:DiagramDescription" name="ComponentModel" domainClass="ComponentModel">
      <defaultLayer name="Default">
        <nodeMappings name="SoftwareComponent" semanticCandidatesExpression="[self.eContents()->select(oclIsKindOf(SoftwareComponent))/]" domainClass="SoftwareComponent">
          <style xsi:type="style:SquareDescription" showIcon="false" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='purple']"/>
          </style>
        </nodeMappings>
        <containerMappings name="HardwareComponent" semanticCandidatesExpression="[self.eContents()->select(oclIsKindOf(&quot;HardwareComponent&quot;))/]" domainClass="HardwareComponent" reusedNodeMappings="//@ownedViewpoints[name='simplecomponent']/@ownedRepresentations[name='ComponentModel']/@defaultLayer/@nodeMappings[name='SoftwareComponent']" reusedContainerMappings="//@ownedViewpoints[name='simplecomponent']/@ownedRepresentations[name='ComponentModel']/@defaultLayer/@containerMappings[name='HardwareComponent']">
          <style xsi:type="style:FlatContainerStyleDescription" showIcon="false">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='green']"/>
          </style>
        </containerMappings>
      </defaultLayer>
    </ownedRepresentations>
  </ownedViewpoints>
</description:Group>
