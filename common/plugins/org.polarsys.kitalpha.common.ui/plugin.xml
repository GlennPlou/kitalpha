<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.polarsys.kitalpha.view.category"
            name="%kitalpha_category">
      </category>
      <category
            id="org.polarsys.kitalpha.view.additional.category"
            name="%kitalpha_additional_category">
      </category>
   </extension>
   
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.polarsys.kitalpha.wizard.category"
            name="%kitalpha_category">
      </category>
   </extension>
   
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.polarsys.kitalpha.ad.wizard.category"
            name="%kitalpha_ad_category"
            parentCategory="org.polarsys.kitalpha.wizard.category">
      </category>
      <category
            id="org.polarsys.kitalpha.wizard.examples.category"
            name="%kitalpha_examples_category"
            parentCategory="org.polarsys.kitalpha.wizard.category">
      </category>
   </extension>
   
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.polarsys.kitalpha.mdeCoreTechnology.wizard.category"
            name="%kitalpha_mde_core_technology_category"
            parentCategory="org.polarsys.kitalpha.wizard.category">
      </category>
   </extension>
   
   <extension
            point="org.eclipse.ui.preferencePages">
         <page
               class="org.polarsys.kitalpha.common.ui.preference.KitalphaPreferencePage"
               id="org.polarsys.kitalpha.preference.category"
               name="%kitalpha_preference">
         </page>
   </extension>
      
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.polarsys.kitalpha.preference.category"
            class="org.polarsys.kitalpha.common.ui.preference.ArchitectureDescriptionPreferencePage"
            id="org.polarsys.kitalpha.ad.preference.category"
            name="%kitalpha_ad_preference">
      </page>
   </extension>
   
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.polarsys.kitalpha.preference.category"
            class="org.polarsys.kitalpha.common.ui.preference.MDECoreTechnologyPreferencePage"
            id="org.polarsys.kitalpha.mdecore.preference.category"
            name="%kitalpha_mde_core_technology_preference">
      </page>
   </extension>

</plugin>
