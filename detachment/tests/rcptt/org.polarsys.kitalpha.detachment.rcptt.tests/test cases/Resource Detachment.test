Entry-Name: .content

let [val model "org.polarsys.kitalpha.vp.componentsample.runtime.model/model/VideoOnDemand.aird"]
	[val detachment "Detachment"]
	[val resourcesPage "Model Resource"]
	[val resourceSectionName "Known Resources"]
	[val projectExplorerViewer "Project Explorer"]
	[val resourceItem "platform:\\/resource\\/org.polarsys.kitalpha.vp.componentsample.runtime.model\\/model\\/VideoOnDemand.componentsample"] {
	
		detachment_openSession $model 

		get-view $projectExplorerViewer | get-tree 
    		| get-item $model 
    		| get-property childCount | equals 2 | verify-true
    
		detachment_closeSession $model
		detachment_launchModelDetachment $model 
		detachment_setVisibleTabItem $resourcesPage

		get-editor $detachment | get-section $resourceSectionName | get-table | get-property itemCount | equals 2 | verify-true
		get-editor $detachment | get-section $resourceSectionName | get-table 
    		| get-item $resourceItem 
    		| uncheck
		get-editor $detachment | get-section $resourceSectionName | get-table 
    		| get-item $resourceItem 
    		| get-property checked | equals false | verify-true

		detachment_performDetachment

		detachment_openSession $model
		get-view $projectExplorerViewer | get-tree 
    		| get-item $model 
    		| get-property childCount | equals 1 | verify-true
    
		detachment_closeSession $model
}
