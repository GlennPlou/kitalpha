proc detachmentScenario_AFViewpointDetachment [val model "org.polarsys.kitalpha.vp.componentsample.runtime.model/model/VideoOnDemand.aird"]
	[val vpSafetyPattern "Component Sample/Component Sample Safety Pattern"]
	[val vpSafetyPatternURI "[http://www.polarsys.org/kitalpha/ComponentSampleSafetyPattern/1.0.0]"]
	[val vpSafety "Component Sample/Component Sample Safety"]
	[val vpSafetyURI "[http://www.polarsys.org/kitalpha/ComponentSampleSafety/1.0.0]"]
	[val vpQualityAssessment "Component Sample/Component Sample Quality Assessment"]
	[val vpQAURI "[http://www.polarsys.org/kitalpha/ComponentSampleQualityAssessment/1.0.0]"]
	[val vpPerformance "Component Sample/Component Sample Performance"]
	[val vpPerfoURI "[http://www.polarsys.org/kitalpha/ComponentSamplePerformance/1.0.0]"]
	[val vpHierarchySection "Viewpoints Hierarchy"]
	[val vpDetachmentPage "Viewpoints Tree Page"]
	[val detachment "Detachment"]
	[val projectExplorerViewer "Project Explorer"] {
	
		detachment_launchModelDetachment -path $model
		detachment_prepareToDetachViewpoint -vpName $vpSafetyPattern -nsURI vpSafetyPatternURI
		detachment_performDetachment

		detachment_launchModelDetachment -path $model
		detachment_prepareToDetachViewpoint -vpName $vpSafety  -nbVpDetected 0 -nsURI vpSafetyURI
		detachment_performDetachment


		detachment_launchModelDetachment -path $model
		detachment_prepareToDetachViewpoint -vpName $vpQualityAssessment --nsURI vpQAURI
		detachment_performDetachment

		detachment_launchModelDetachment -path $model
		detachment_prepareToDetachViewpoint -vpName $vpPerformance -nsURI vpPerfoURI
		detachment_performDetachment

		detachment_launchModelDetachment -path $model
		detachment_setVisibleTabItem $vpDetachmentPage

		get-editor $detachment | get-section $vpHierarchySection | get-tree | get-property itemCount | equals 1 
    		| verify-true
		get-editor $detachment | close

		detachment_openSession $model 
		with [get-editor "Video On Demande - Activity Explorer"] {
    		get-link "" | click
    		get-section "Diagrams Viewer" | get-tree 
        		| select "ComponentSample/ComponentSampleDiagram/Video on demand Architecture" | double-click
		}
		get-view $projectExplorerViewer | get-tree | select $model | get-menu Close | click
		get-window Save | get-button Yes | click
}

detachmentScenario_AFViewpointDetachment
