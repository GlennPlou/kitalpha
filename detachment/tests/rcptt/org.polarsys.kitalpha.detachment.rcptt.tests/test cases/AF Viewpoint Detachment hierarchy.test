let [val model "org.polarsys.kitalpha.vp.componentsample.runtime.model/model/VideoOnDemand.aird"]
	[val vpSafety "Component Sample/Component Sample Safety"]
	[val vpQualityAssessment "Component Sample/Component Sample Quality Assessment"]
	[val vpPerformance "Component Sample/Component Sample Performance"]
	[val vpHierarchySection "Viewpoints Hierarchy"]
	[val vpDetachmentPage "Viewpoints Tree Page"]
	[val detachment "Detachment"]
	[val projectExplorerViewer "Project Explorer"] {
	
		detachment_launchModelDetachment -path $model
		detachment_prepareToDetachViewpoint -vpName $vpSafety -nbVpDetected 1
		detachment_performDetachment


		detachment_launchModelDetachment -path $model
		detachment_prepareToDetachViewpoint -vpName $vpQualityAssessment
		detachment_performDetachment

		detachment_launchModelDetachment -path $model
		detachment_prepareToDetachViewpoint -vpName $vpPerformance
		detachment_performDetachment

		detachment_launchModelDetachment -path $model
		detachment_setVisibleTabItem $vpDetachmentPage

		get-editor $detachment | get-section $vpHierarchySection | get-tree | get-property itemCount | equals 1 
    		| verify-true
		get-editor $detachment | close

		detachment_openSession $model
		wait 3000
	    get-editor "Video On Demande - Activity Explorer" | get-tab-folder | get-tab-item "Component Sample Architecture" | click
		with [get-editor "Video On Demande - Activity Explorer"] {
    		get-section "Diagrams Viewer" | get-tree 
        	| select "ComponentSample/ComponentSampleDiagram/Video on demand Architecture" | double-click
		}
		get-view $projectExplorerViewer | get-tree | select $model | get-menu Close | click
		get-window Save | get-button Yes | click
}
